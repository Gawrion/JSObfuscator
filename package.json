{
	"name": "JSObfuscator",
	"displayName": "JSObfuscator",
	"publisher": "SoftHeroes",
	"repository": "https://github.com/SoftHeroes/JSObfuscator",
	"icon": "images/logo.png",
	"description": "Code Obfuscator, javaScript Obfuscator,Obfuscator. This extension will convert all your JavaScript Code in minifier version and also Obfuscator them to make it secure.Thanks for using :)",
	"version": "1.0.2",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.JSObfuscatorEncodeJSCode"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.JSObfuscatorEncodeJSCode",
				"title": "JSObfuscator : Encode JS Code"
			}
		],
		"configuration": {
			"type": "object",
			"title": "JSObfuscator Settings",
			"properties": {
				"JSObfuscator.ignoreMinFiles": {
					"type": "boolean",
					"default": true,
					"markdownDescription": "**No Obfuscator for .min.js**\nYes or no?"
				},
				"JSObfuscator.subPathInWorkspace": {
					"type": "string",
					"default": "",
					"description": "Specified some relative path where you only want to Obfuscate JavaScript"
				},
				"JSObfuscator.ignoreFile": {
					"type": "string",
					"default": "",
					"description": "Enter file name with comma separated"
				}
			}
		}
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"eslint": "^6.6.0",
		"glob": "^7.1.5",
		"javascript-obfuscator": "^0.19.2",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"javascript-obfuscator": "^0.19.2"
	}
}